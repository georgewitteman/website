{
  "extends": "../tsconfig.base.json",
  "include": [
    "**/*.js",
    "**/*.cjs",
    "**/*.mjs",
    "**/*.ts",
    "**/.*js",
    "**/.*.cjs",
    "**/.*.mjs"
  ],
  "exclude": [],
  "compilerOptions": {
    "module": "ES2022",
    "moduleDetection": "force",
    "moduleResolution": "node16",
    "lib": ["DOM"]
  }
}
